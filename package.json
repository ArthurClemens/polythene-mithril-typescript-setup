{
  "name": "polythene-mithril-typescript-setup",
  "version": "1.0.0",
  "description": "Polythene for Mithril with TypeScript setup example",
  "license": "MIT",
  "scripts": {
    "dev": "npm-run-all --parallel dev:watch dev:serve",
    "dev:serve": "webpack-dev-server --config scripts/webpack.config.dev.js --port 3000",
    "dev:watch": "webpack --watch --config scripts/webpack.config.dev.js",
    "build": "webpack --config scripts/webpack.config.prod.js"
  },
  "dependencies": {
    "mithril": "1.1.6",
    "@types/mithril": "1.1.12",
    "polythene-css": "1.2.0",
    "polythene-mithril": "1.2.0"
  },
  "devDependencies": {
    "@types/node": "^10.3.3",
    "compression-webpack-plugin": "1.1.11",
    "css-loader": "0.28.11",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "npm-run-all": "4.1.3",
    "style-loader": "0.21.0",
    "ts-loader": "4.4.1",
    "typescript": "2.9.2",
    "typings": "^2.1.1",
    "uglifyjs-webpack-plugin": "1.2.5",
    "webpack": "4.12.0",
    "webpack-cli": "3.0.6",
    "webpack-dev-server": "3.1.4"
  }
}
